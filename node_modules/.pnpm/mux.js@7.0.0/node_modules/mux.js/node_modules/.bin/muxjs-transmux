#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/noah/MyWallet/starter-kit/node_modules/.pnpm/mux.js@7.0.0/node_modules/mux.js/bin/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/mux.js@7.0.0/node_modules/mux.js/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/mux.js@7.0.0/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/noah/MyWallet/starter-kit/node_modules/.pnpm/mux.js@7.0.0/node_modules/mux.js/bin/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/mux.js@7.0.0/node_modules/mux.js/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/mux.js@7.0.0/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/transmux.js" "$@"
else
  exec node  "$basedir/../../bin/transmux.js" "$@"
fi
