#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/noah/MyWallet/starter-kit/node_modules/.pnpm/case-police@0.6.1/node_modules/case-police/bin/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/case-police@0.6.1/node_modules/case-police/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/case-police@0.6.1/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/noah/MyWallet/starter-kit/node_modules/.pnpm/case-police@0.6.1/node_modules/case-police/bin/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/case-police@0.6.1/node_modules/case-police/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/case-police@0.6.1/node_modules:/home/noah/MyWallet/starter-kit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../case-police/bin/case-police.mjs" "$@"
else
  exec node  "$basedir/../case-police/bin/case-police.mjs" "$@"
fi
